<div id="#reviews">
  <div class=" card card-review review">
    <div class="review-comment">
      <p>Comment:</p>
      <%= request.booking.review.comment %>
    </div>
  </div>
  <div class="review-info">
    <div class="review-author">
      <%= link_to "#{request.client.first_name}" %>
    </div>
    <div class="review-rating">
      <i class="fa fa-star star-review"></i> <%= request.booking.review.rating %>
    </div>
  </div>
</div>

