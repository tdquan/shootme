<%= simple_form_for [user, booking, @review] do |f| %>
  <%= f.input :comment, label: false, as: :text, class: 'review_form' %>
  <%= f.select :rating, [1, 2, 3, 4, 5 ]%>
  <%= f.submit "Leave Review", class: "btn btn-primary center-block", data: { target: "#newReview", dismiss: "modal" } %>
<% end %>
